{
  "env": {
    "commonjs": {
      "presets": ["env"],
      "plugins": [
        "add-module-exports",
        "transform-object-rest-spread"
      ]
    },
    "es": {
      "presets": [
        ["env", {
        "modules": false,
        "loose": true,
        "targets": {
          "node": 6
        }
        }
        ]],
      "plugins": [
        "transform-object-rest-spread"
      ]
    }
}
}
