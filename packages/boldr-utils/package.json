{
  "name": "boldr-utils",
  "version": "0.1.14",
  "description": "Helpers designed for use with Boldr and fullstack react apps",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/boldr/getBoldr.git"
  },
  "scripts": {
    "build": "npm run build:commonjs && npm run build:es",
    "clean": "rimraf dist es",
    "prepublish": "npm run clean && npm run build",
    "build:commonjs": "BABEL_ENV=commonjs babel --plugins add-module-exports src --out-dir dist",
    "build:es": "BABEL_ENV=es babel src --out-dir es",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "boldr",
    "react",
    "webpack",
    "dev",
    "helpers"
  ],
  "main": "dist/index.js",
  "jsnext:main": "es/index.js",
  "files": [
    "dist",
    "es",
    "src"
  ],
  "author": "Steven Truesdell <steven@strues.io>",
  "license": "MIT",
  "dependencies": {
    "chalk": "^1.1.3",
    "lodash.reduce": "^4.6.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-eslint": "^7.2.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.2.2",
    "eslint": "^3.18.0",
    "eslint-config-boldr": "^0.8.9",
    "rimraf": "^2.6.1"
  }
}
