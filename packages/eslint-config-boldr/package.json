{
  "name": "eslint-config-boldr",
  "version": "0.8.2",
  "description": "ESLint rules for Boldr.",
  "main": "index.js",
  "files": [
    "index.js",
    "react.js"
  ],
  "scripts": {
    "find-new-rules": "eslint-find-rules --unused ./index.js",
    "find-new-rules:react": "eslint-find-rules --unused ./react.js",
    "lint": "eslint .",
    "test": "npm-run-all --parallel lint find-new-rules"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/boldr/getBoldr.git"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "config",
    "react",
    "javascript",
    "styleguide"
  ],
  "author": "Steven Truesdell <steven@strues.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/boldr/getBoldr/issues"
  },
  "homepage": "https://github.com/boldr/getBoldr#readme",
  "engines": {
    "node": ">= 7"
  },
  "peerDependencies": {
    "eslint": "^3.17.1"
  },
  "devDependencies": {
    "eslint": "^3.17.1",
    "eslint-find-rules": "^1.14.3",
    "npm-run-all": "^4.0.2"
  },
  "dependencies": {
    "babel-eslint": "^7.1.1",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-flowtype": "^2.30.3",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "6.10.0"
  }
