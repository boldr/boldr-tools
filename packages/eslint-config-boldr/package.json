{
  "name": "eslint-config-boldr",
  "version": "0.9.1",
  "description": "ESLint rules for Boldr.",
  "main": "index.js",
  "files": [
    "index.js",
    "react.js",
    "rules/best-practices.js",
    "rules/stylistic.js",
    "rules/es6.js",
    "rules/node.js",
    "rules/possible-errors.js",
    "flowtype.js",
    "import.js",
    "jsx-a11y.js"
  ],
  "scripts": {
    "find-new-rules": "eslint-find-rules --unused ./index.js",
    "lint": "eslint .",
    "test": "npm-run-all --parallel lint find-new-rules"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/boldr/boldr-tools.git"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "config",
    "react",
    "javascript",
    "styleguide"
  ],
  "author": "Steven Truesdell <steven@strues.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/boldr/boldr-tools/issues"
  },
  "homepage": "https://github.com/boldr/boldr-tools#readme",
  "engines": {
    "node": ">= 7"
  },
  "peerDependencies": {
    "eslint": "^3.19.0",
    "prettier": "^1.1.0"
  },
  "devDependencies": {
    "cz-conventional-changelog": "^2.0.0",
    "eslint": "^3.19.0",
    "eslint-find-rules": "^1.14.3",
    "npm-run-all": "^4.0.2"
  },
  "dependencies": {
    "babel-eslint": "^7.2.2",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-flowtype": "^2.30.4",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-prettier": "^2.0.1",
    "eslint-plugin-react": "6.10.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
