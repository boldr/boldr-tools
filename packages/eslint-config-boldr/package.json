  "homepage": "https://github.com/boldr/boldr-tools#readme",
  "engines": {
    "node": ">= 7"
  },
  "peerDependencies": {
    "eslint": "^3.19.0",
    "prettier": "^1.1.0"
  },
  "devDependencies": {
    "eslint": "^3.19.0",
    "eslint-config-prettier": "^2.1.0",
    "eslint-find-rules": "^1.14.3",
    "npm-run-all": "^4.0.2"
  },
  "dependencies": {
