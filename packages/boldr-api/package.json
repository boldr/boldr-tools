{
  "name": "boldr-api",
  "version": "0.1.0",
  "description": "API server and database for Boldr CMS",
  "main": "index.js",
  "license": "MIT",
  "author": "Steven Truesdell <steven@strues.io>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/boldr/getBoldr.git"
  },
  "bugs": "https://github.com/boldr/getBoldr/issues",
  "homepage": "https://github.com/boldr/getBoldr#readme",
  "scripts": {
    "dev": "better-npm-run dev",
    "migrate": "better-npm-run migrate",
    "migrate:test": "better-npm-run migrate:test",
    "migration": "better-npm-run migration",
    "seed": "better-npm-run seed",
    "seed:test": "better-npm-run seed:test"
  },
  "dependencies": {
    "app-root-dir": "^1.0.2",
    "bcryptjs": "^2.4.3",
    "better-npm-run": "^0.0.14",
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.1",
    "boldr-utils": "0.1.12",
    "busboy": "^0.2.14",
    "chalk": "^1.1.3",
    "compression": "^1.6.2",
    "connect-busboy": "^0.0.2",
    "connect-redis": "^3.2.0",
    "convict": "^2.0.0",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.1",
    "date-fns": "^1.28.1",
    "debug": "^2.6.3",
    "dotenv": "4.0.0",
    "express": "4.15.2",
    "express-flash": "^0.0.2",
    "express-redis-cache": "^0.5.0",
    "express-session": "^1.15.1",
    "express-validator": "^3.1.2",
    "express-winston": "^2.3.0",
    "fs-extra": "2.1.2",
    "gm": "^1.23.0",
    "helmet": "3.5.0",
    "hpp": "^0.2.1",
    "ioredis": "^2.5.0",
    "jsonwebtoken": "^7.3.0",
    "knex": "0.12.8",
    "lodash": "^4.17.4",
    "method-override": "^2.3.7",
    "moment": "^2.18.0",
    "morgan": "^1.8.1",
    "nodemailer": "^3.1.7",
    "objection": "0.7.9",
    "objection-find": "^0.4.0",
    "pg": "^6.1.4",
    "shortid": "^2.2.8",
    "uuid": "^3.0.1",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-eslint": "^7.1.1",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.2.2",
    "cross-env": "^3.2.4",
    "eslint": "^3.18.0",
    "eslint-config-boldr": "^0.8.8",
    "eslint-plugin-flowtype": "^2.30.3",
    "fast-async": "^6.2.1",
    "flow-bin": "^0.42.0",
    "nodemon": "^1.11.0",
    "rimraf": "^2.6.1"
  },
  "keywords": [
    "boldr",
    "cli",
    "boldr-cli"
  ],
  "betterScripts": {
    "dev": {
      "command": "cross-env nodemon --exec babel-node ./src/index.js",
      "env": {
        "NODE_ENV": "development",
        "DATABASE_URI": "postgres://postgres:password@localhost:5432/boldr",
        "REDIS_URI": "redis://127.0.0.1:6379/1",
        "DATABASE_DEBUG": true,
        "DEBUG": "boldrAPI:*"
      }
    },
    "migrate": {
      "command": "node --harmony internal/scripts/db.js migrate",
      "env": {
        "DATABASE_URI": "postgres://postgres:password@localhost:5432/boldr",
        "NODE_ENV": "development"
      }
    },
    "migrate:test": {
      "command": "node --harmony internal/scripts/db.js migrate",
      "env": {
        "DATABASE_URI": "postgres://postgres:password@localhost:5432/boldr_test",
        "NODE_ENV": "development"
      }
    },
    "migration": {
      "command": "node --harmony internal/scripts/db.js migration",
      "env": {
        "DATABASE_URI": "postgres://postgres:password@localhost:5432/boldr",
        "NODE_ENV": "development"
      }
    },
    "migrate:undo": {
      "command": "node --harmony internal/scripts/db.js migrate:undo",
      "env": {
        "DATABASE_URI": "postgres://postgres:password@localhost:5432/boldr",
        "NODE_ENV": "development"
      }
    },
    "seed": {
      "command": "knex seed:run",
      "env": {
        "DATABASE_URI": "postgres://postgres:password@localhost:5432/boldr",
        "NODE_ENV": "development"
      }
    },
    "seed:test": {
      "command": "knex seed:run",
      "env": {
        "DATABASE_URI": "postgres://postgres:password@localhost:5432/boldr_test",
        "NODE_ENV": "development"
      }
    }
  }
}
