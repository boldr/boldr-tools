{
  "name": "boldr-dx",
  "version": "0.2.3",
  "description": "Webpack and build toolkit for Boldr development",
  "license": "MIT",
  "author": "Steven Truesdell <steven@strues.io>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/boldr/boldr-tools.git"
  },
  "bugs": {
    "url": "https://github.com/boldr/boldr-tools/issues"
  },
  "homepage": "https://github.com/boldr/boldr-tools#readme",
  "bin": {
    "boldr-dx": "./dist/boldr-dx.js"
  },
  "main": "dist/boldr-dx.js",
  "jsnext:main": "es/boldr-dx.js",
  "files": [
    "dist",
    "es",
    "src"
  ],
  "engines": {
    "node": ">=6.1.0"
  },
  "scripts": {
    "build": "npm run clean && npm run build:commonjs && npm run build:es",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel --plugins add-module-exports src --out-dir dist",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es",
    "clean": "rimraf dist && rimraf es",
    "prepublish": "npm run clean && npm run build"
  },
