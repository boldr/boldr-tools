    "format": "prettier --single-quote=true --jsx-bracket-same-line=false --trailing-comma=all --write \"src/**/*.js\"",
    "test": "echo \"No tests\" && exit 0"
  },
  "dependencies": {
    "app-root-dir": "^1.0.2",
    "babel-jest": "^20.0.1",
    "babel-preset-boldr": "^0.8.0",
    "bluebird": "^3.5.0",
    "boldr-utils": "^0.2.3",
    "chalk": "^1.1.3",
    "commander": "^2.9.0",
    "cross-env": "^5.0.0",
    "cross-spawn": "^5.1.0",
    "debug": "^2.6.6",
    "enzyme-to-json": "^1.5.1",
    "eslint": "^3.19.0",
    "eslint-config-boldr": "^0.9.5",
    "fs-extra": "^3.0.1",
